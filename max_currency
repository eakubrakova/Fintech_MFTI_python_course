import requests

def get_max_currency():
    # Запрос к API для получения данных о курсах валют
    response = requests.get('https://www.cbr-xml-daily.ru/daily_json.js')
    
    # Проверка успешности запроса
    if response.status_code != 200:
        return 'Ошибка при получении данных'
    
    # Получение данных в формате JSON
    data = response.json()
    
    # Поиск валюты с максимальным значением курса
    max_rate = 0
    max_currency = ''
    for currency, info in data['Valute'].items():
        if info['Value'] > max_rate:
            max_rate = info['Value']
            max_currency = info['Name']
    
    return max_currency

print(get_max_currency())
